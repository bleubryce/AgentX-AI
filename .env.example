# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/subscription_db

# Redis Configuration
REDIS_URL=redis://localhost:6379/0

# Stripe API Keys
STRIPE_API_KEY=sk_test_your_stripe_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# JWT Authentication
JWT_SECRET=your_jwt_secret_key
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30
JWT_REFRESH_TOKEN_EXPIRE_DAYS=7

# CORS Settings
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com

# Email Configuration
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_email@example.com
SMTP_PASSWORD=your_email_password
EMAILS_FROM_EMAIL=noreply@example.com
EMAILS_FROM_NAME=Subscription Service

# Logging
LOG_LEVEL=INFO

# Frontend Configuration
REACT_APP_API_URL=http://localhost:8000
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key

# API Keys
OPENAI_API_KEY=your_openai_api_key
ZILLOW_API_KEY=your_zillow_api_key
REALTOR_API_KEY=your_realtor_api_key
ATTOM_API_KEY=your_attom_api_key
RENTCAST_API_KEY=your_rentcast_api_key

# RentCast API Configuration
RENTCAST_RATE_LIMIT=5
RENTCAST_USE_CACHE=true
RENTCAST_CACHE_DAYS=30

# Web Scraping Configuration
PROXY_SERVICE=your_proxy_service_url
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36 